{
  "name": "node-server-seed",
  "version": "1.0.0",
  "description": "A production ready server created by Nodejs with a series of helpful pre-configured functionalities, eg. scheduler, HTTPS cert creation, send text message, send email, write & parse csv, database interaction (Oracle, PostgreSQL, Firebase), etc.",
  "scripts": {
    "prestart": "npm run build",
    "start": "cross-env NODE_ENV=production node ./server/index.js",
    "build": "webpack --config webpack.prod.js",
    "dev": "cross-env NODE_ENV=dev npm-run-all --parallel watch:build  watch:server ",
    "debug": "cross-env NODE_ENV=debug npm-run-all --parallel watch:build watch:server ",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch:build": "webpack --watch --config webpack.dev.js",
    "watch:server": "nodemon \"./server/index.js\" --watch \"./dist\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jefftham/node-server-seed.git"
  },
  "keywords": [
    "node",
    "node server",
    "server",
    "http",
    "server",
    "https",
    "server",
    "letsenscript",
    "seed",
    "node-server-seed"
  ],
  "author": "Jeff Tham <Jeff.Tham@live.com>",
  "license": "MIT",
